<%- include('../includes/head.ejs') %>

<link rel="stylesheet" href="/css/std_attendance.css">

</head>

<body>
  <%- include('../includes/navigation-admin.ejs') %>

  <% for (let name in students) { %>
    <% if (students[name].rollno) { %>
  <main>

   
        <div class="list">
          <ul >
            <h3> Name : <%= name %></h3>
            <li> Roll number : <%= students[name].rollno %></li>
            <li> Attended : <%= students[name].attended %></li>
            <li> Delivered : <%= students.delivered %></li>
            <li> Percentage : <%= students[name].percentage + "%" %></li>
          </ul>
        </div>
      
  </main>
  <% } %>
   <% } %>
    
</body>
</html>